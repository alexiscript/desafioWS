<h2>Agregar Producto</h2>
<form id="formulario">
    <div class="m-3">
        <label for="title">Nombre</label>
        <input type="text" class="form-control" id="title" placeholder="Ingrese nombre producto">
    </div>
    <div class="m-3">
        <label for="description">Descripcion</label>
        <input type="text" class="form-control" id="description" placeholder="Ingrese descripcion producto">
    </div>
    <div class="m-3">
        <label for="price">Precio</label>
        <input type="number" class="form-control" id="price" placeholder="Ingrese Precio producto">
    </div>
    <div class="m-3">
        <label for="code">Codigo SKU</label>
        <input type="text" class="form-control" id="code" placeholder="Ingrese codigo producto">
    </div>
    <div class="m-3">
        <label for="stock">Stock</label>
        <input type="number" class="form-control" id="stock" placeholder="Ingrese Stock producto">
    </div>
    <div class="m-3">
         <input type="text" class="form-control" id="category" placeholder="Ingrese Categoria producto">
    </div>
    <button type="submit" class="btn btn-primary m-2">Enviar</button>
</form>
<div id="products-container" class="d-flex"></div>
<script src='/socket.io/socket.io.js'></script>
<script src="/js/index.js"></script>